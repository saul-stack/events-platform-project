{
  "schema": {
    "tableName": "events",
    "columns": [
      { "name": "id", "type": "SERIAL PRIMARY KEY" },
      { "name": "title", "type": "VARCHAR(255) NOT NULL UNIQUE" },
      { "name": "date", "type": "DATE NOT NULL" },
      { "name": "time", "type": "TIME NOT NULL" },
      { "name": "description", "type": "TEXT NOT NULL" },
      {
        "name": "advance_price",
        "type": "MONEY NOT NULL CHECK (advance_price::numeric >= 0)"
      },
      {
        "name": "door_price",
        "type": "MONEY NOT NULL CHECK (door_price::numeric >= 0)"
      },
      {
        "name": "tickets_total",
        "type": "INT NOT NULL DEFAULT 0 CHECK (tickets_total >= 0)"
      },
      {
        "name": "tickets_sold",
        "type": "INT NOT NULL DEFAULT 0 CHECK (tickets_sold >= 0)"
      },
      { "name": "is_seated", "type": "BOOLEAN NOT NULL" },
      { "name": "is_ticketed", "type": "BOOLEAN NOT NULL" },
      { "name": "is_recurring", "type": "BOOLEAN NOT NULL" },
      { "name": "image_url", "type": "TEXT" }
    ]
  },
